{
    "compilerOptions": {
        "target": "es2024",
        "module": "node18",
        "outDir": "./build",
        "rootDir": "./src",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": false,
        "forceConsistentCasingInFileNames": true
    },
    "include": [ "src/**/*"],
    "exclude": [ "node_modules" ]
}